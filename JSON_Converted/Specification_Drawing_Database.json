                    ],
                    [
                        "Parent",
                        "A reference to another [MenuID] if this menu has a parent."
                    ],
                    [
                        "ItemNumber",
                        "The sort order for a group of menus."
                    ],
                    [
                        "Label",
                        "The Name of the Symbol."
                    ],
                    [
                        "SceneID",
                        "A cross-reference to the [Scenes].[ SceneID] field."
                    ],
                    [
                        "Type",
                        "Indicates the template type for the drawing symbol \n 0=None \n 1=Title Block \n 2=Assembly Sheets \n 3=Wall Elevations \n 4=Floor Plans \n 5=Drawing Scenes"
                    ]
